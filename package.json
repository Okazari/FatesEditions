{
  "name": "workspace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "cd ./ui/ && node scripts/build.js",
    "lint": "eslint ui/src/**/*.js",
    "eslint": "eslint --fix",
    "dev": "concurrently -p name -n \"MONGO,BACK,FRONT\" \"npm run startDevMongo\" \"npm run startDevBack\" \"npm run startDevFront\" ",
    "devWindows": "concurrently -p name -n \"MONGO,BACK,FRONT\" \"npm run startDevWindowsMongo\" \"npm run startDevBack\" \"npm run startDevFront\" ",
    "startDevBack": "nodemon ./bin/www",
    "startDevFront": "cd ./ui/ && node scripts/start.js",
    "startDevMongo": "docker-compose up -d",
    "startDevWindowsMongo": "docker run --rm -p 27017:27017 mongo:3.2.6",
    "postinstall": "npm run build"
  },
